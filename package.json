{
  "name": "alpheios-embedded",
  "version": "0.3.0",
  "description": "Alpheios Embedded Library",
  "main": "dist/alpheios-embedded.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "standard --fix && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all all vue",
    "build-prod": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all production vue",
    "build-dev": "standard --fix && node --experimental-modules ./node_modules/alpheios-node-build/dist/build.mjs all development vue",
    "lint": "standard --fix",
    "dev": "npm run build-dev && http-server -c-1 -p 8888 & onchange src -- npm run build-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/wordsvc.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/wordsvc/issues"
  },
  "homepage": "https://github.com/alpheios-project/wordsvc#readme",
  "devDependencies": {
    "alpheios-components": "git://github.com/alpheios-project/components.git#infl-games",
    "alpheios-data-models": "git://github.com/alpheios-project/data-models.git",
    "alpheios-experience": "github:alpheios-project/experience",
    "alpheios-inflection-games": "git://github.com/alpheios-project/inflection-games.git",
    "alpheios-inflection-tables": "git://github.com/alpheios-project/inflection-tables.git",
    "alpheios-lemma-client": "git://github.com/alpheios-project/lemma-client.git",
    "alpheios-lexicon-client": "github:alpheios-project/lexicon-client",
    "alpheios-morph-client": "git://github.com/alpheios-project/morph-client.git",
    "alpheios-node-build": "github:alpheios-project/node-build",
    "alpheios-res-client": "github:alpheios-project/res-client",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-loader-jest": "^0.2.1",
    "http-server": "^0.11.1",
    "interactjs": "^1.3.4",
    "intl-messageformat": "^2.2.0",
    "jest": "^22.4.4",
    "jest-fetch-mock": "1.3.3",
    "onchange": "^2.4.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.1.1",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-svg-loader": "^0.5.0",
    "webpack-dev-server": "^3.1.7"
  },
  "engines": {
    "node": ">= 9.10.1",
    "npm": ">= 5.6.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jest-environment-jsdom-11.0.0",
    "transform": {
      "^.+\\.htmlf$": "html-loader-jest",
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/alpheios-components/"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    }
  },
  "standard": {
    "ignore": [
      "**/dist",
      "**/support"
    ]
  },
  "dependencies": {
    "node-sass": "^4.9.3"
  }
}
