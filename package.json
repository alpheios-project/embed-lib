  "devDependencies": {
    "alpheios-components": "git://github.com/alpheios-project/components.git",
    "alpheios-data-models": "git://github.com/alpheios-project/data-models.git",
    "alpheios-experience": "git://github.com/alpheios-project/experience.git",
    "alpheios-inflection-tables": "git://github.com/alpheios-project/inflection-tables.git",
    "alpheios-lexicon-client": "git://github.com/alpheios-project/lexicon-client.git",
    "alpheios-res-client": "git://github.com/alpheios-project/res-client.git",
